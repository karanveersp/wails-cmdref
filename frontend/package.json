{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "prepare": "pnpm run smui-theme-light && pnpm run smui-theme-dark",
        "smui-theme-light": "smui-theme compile src/smui.css -i src/theme -i ./node_modules",
        "smui-theme-dark": "smui-theme compile src/smui-dark.css -i src/theme/dark -i ./node_modules"
    },
    "devDependencies": {
        "@material/elevation": "^14.0.0",
        "@material/theme": "^14.0.0",
        "@smui-extra/autocomplete": "7.0.0-beta.14",
        "@smui/button": "7.0.0-beta.14",
        "@smui/card": "7.0.0-beta.14",
        "@smui/common": "7.0.0-beta.14",
        "@smui/drawer": "7.0.0-beta.14",
        "@smui/fab": "7.0.0-beta.14",
        "@smui/icon-button": "7.0.0-beta.14",
        "@smui/layout-grid": "7.0.0-beta.14",
        "@smui/list": "7.0.0-beta.14",
        "@smui/paper": "7.0.0-beta.14",
        "@smui/textfield": "7.0.0-beta.14",
        "@sveltejs/vite-plugin-svelte": "^1.0.1",
        "@tsconfig/svelte": "^3.0.0",
        "smui-theme": "7.0.0-beta.14",
        "svelte": "^3.55.1",
        "svelte-check": "^2.8.0",
        "svelte-material-ui": "7.0.0-beta.14",
        "svelte-preprocess": "^4.10.7",
        "tslib": "^2.4.0",
        "typescript": "^4.9.4",
        "vite": "^3.0.0"
    }
}
